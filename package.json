{
  "name": "nine-dart-nation",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "node scripts/dev.cjs",
    "prepare": "husky install",
    "build": "vite build",
    "build:server": "node ./scripts/build-server.cjs",
    "build:all": "npm run build && npm run build:server",
    "preview": "vite preview",
    "prestart": "npm run build:server",
    "start": "node server/server.cjs",
    "e2e:pair": "node scripts/e2e_pairing.cjs",
    "e2e:calibrate": "node scripts/e2e_calibration.cjs",
    "e2e:ui-smoke": "node scripts/e2e_ui_smoke.cjs",
    "test:redis": "node scripts/test_redis.cjs",
    "test:unit": "cross-env NODE_NO_WARNINGS=1 vitest run",
    "test:unit:quiet": "cross-env NODE_NO_WARNINGS=1 vitest run --reporter=dot",
    "test:integration": "cross-env NODE_NO_WARNINGS=1 NDN_RUNINTEGRATION=1 vitest run src/server/__tests__/autocommit.integration.test.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "map-symbol": "node scripts/map-symbol.cjs",
    "generate:icons": "node scripts/generate-icons.cjs"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.75.0",
    "@upstash/redis": "^1.35.6",
    "@vitejs/plugin-react": "^4.3.1",
    "bad-words": "^3.0.4",
    "bcrypt": "^6.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.0.0",
    "js-aruco": "^0.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.453.0",
    "nanoid": "^5.0.7",
    "nodemailer": "^7.0.10",
    "pg": "^8.16.3",
    "pino": "^9.4.0",
    "pino-http": "^10.3.0",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-focus-lock": "^2.9.0",
    "redis": "^5.9.0",
    "stripe": "^16.0.0",
    "vite": "^7.1.12",
    "ws": "^8.18.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "autoprefixer": "^10.4.20",
    "baseline-browser-mapping": "^2.9.4",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^8.0.3",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.47",
    "prettier": "^3.5.1",
    "puppeteer": "^24.27.0",
    "sharp": "^0.32.4",
    "source-map": "^0.7.4",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "vitest": "^4.0.5"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint:fix --",
      "git add"
    ]
  }
}
